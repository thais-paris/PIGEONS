<div class="container fullheight mt-4 p-5">
  <div class="row">
    <div class="col-12 col-sm-10 col-lg-8 col-xl-6 m-auto">
      <h2>Editer mon profil</h2>

        <%= render "reg-form" %>

      <div class="text-warning d-flex flex-column w-100 align-items-end">
        <%= link_to "Supprimer mon profil", registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete, class:'shared-link' %>
        <%= link_to "Retour", :back, class: "shared-link" %>
      </div>
    </div>
  </div>
</div>
